<?xml version="1.0" encoding="ISO-8859-1"?>

<config>
    
    <component name="startAardvark" type="com.sun.labs.aura.grid.StartAardvark"/>
    
    <component name="stopAardvark" type="com.sun.labs.aura.grid.StopAardvark"/>
    
    <component name="start" type="com.sun.labs.aura.AuraServiceStarter">
        <propertylist name="serviceComponents">
            <item>startAardvark</item>
        </propertylist>
        <property name="blockForShutdown" value="false"/>
    </component>
    
    <component name="stop" type="com.sun.labs.aura.AuraServiceStarter">
        <propertylist name="serviceComponents">
            <item>stopAardvark</item>
        </propertylist>
        <property name="blockForShutdown" value="false"/>
    </component>
</config>